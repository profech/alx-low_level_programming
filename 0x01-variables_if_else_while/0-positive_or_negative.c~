#include  <stdlib.h> 
#include  <time.h> 
#include  <stdio.h>
/**
 *main - entry point function
 *
 *return - returns 0
 *
 */
int main(void)
{
int n;
srand(time(0));
n = rand() - RAND_MAX / 2;
if (n < 0)
{
printf("%d is negative (less than Zero)\n", n);
}
else if (n == 0)
{
printf("%d is zero\n", n);
}
else
{
printf("%d is positive (Greater than Zero)\n", n);
}
return (0);
}
